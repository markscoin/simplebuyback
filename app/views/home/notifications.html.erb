<div class="pad45"></div>
<div class="container">
  <div class="col-xs-8 col-xs-offset-2 table-bordered table">
    <h1>Detailed List of Items</h1>
    <table style="width: 600px;" >
      <tr>
        <th>Variant #</th>
        <th>Product #</th>
        <th>Name</th>
        <th>Condition</th>
        <th>Price</th>
      </tr>
        <% @completefinal.each do |x| %>
        <tr class="center">
          <td><%= x[:variant_id]%></td>
          <td><%= x[:id] %></td>
          <td><%= x[:name] %></td>
          <td><%= x[:condition] %></td>
          <td>$ <%= x[:price] %></td>
        </tr>
        <% end %>

    </table>
  </div>
</div>

